#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules/vitepress/bin/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules/vitepress/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules/vitepress/bin/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules/vitepress/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_29fe467bb40485a8b2473183e9d59c66/node_modules:/mnt/d/lnk/开发/corli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
